  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "modern-normalize": "^1.1.0",
    "node-polyfill-webpack-plugin": "^2.0.0",
    "normalize.css": "^8.0.1",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.14",
    "simple-functional-loader": "^1.2.1",
    "tailwindcss": "^3.1.6",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "copy-webpack-plugin": "^11.0.0"
  }
