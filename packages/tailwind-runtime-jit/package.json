{
  "name": "tailwind-runtime-jit",
  "version": "0.1.0",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./src/index.ts"
    },
    "./webpack": {
      "default": "./lib/webpack.js",
      "require": "./dist/webpack.js"
    },
